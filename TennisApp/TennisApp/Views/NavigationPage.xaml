<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TennisApp.Views.NavigationPage">
    <ContentPage.Content>
        <StackLayout Grid.Row="1" Grid.Column="2">
            <Grid>
                <Button WidthRequest="54" Text="Toernooi" BackgroundColor="LightGreen" Command="{Binding ToernooiPageCommand}"></Button>
            </Grid>
            <Grid>
                <Button WidthRequest="54" Text="Spelers" BackgroundColor="LightGreen" Command="{Binding SpelersLijstPageCommand}"></Button>
            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>